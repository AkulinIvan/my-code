def gen_bin(M, prefix=''):
    if M == 0:
        print(prefix)
        return    
    gen_bin(M-1, prefix+'0')
    gen_bin(M-1, prefix+'1')
def generate_numbers(N:int, M: int, prefix = None):
    if M == 0: 
        print(prefix)
        return
    prefix = prefix or []
    for digit in range(N):
        prefix.append(digit)
        generate_numbers(N, M-1, prefix)
        prefix.pop()
#generate_numbers(2, 2)
gen_bin(5)
 
